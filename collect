while getgenv().Toggle do
    local char = game:GetService("Players").LocalPlayer.Character
    if char then
        for _, tool in pairs(char:GetChildren()) do
            if string.find(tool.Name, "Pan") then
                local scriptsFolder = tool:FindFirstChild("Scripts")
                if scriptsFolder then
                    local toggleShovelActive = scriptsFolder:FindFirstChild("ToggleShovelActive")
                    if toggleShovelActive then
                        toggleShovelActive:FireServer(true)
                    end
                    task.wait(0.5)
                    local collect = scriptsFolder:FindFirstChild("Collect")
                    if collect then
                        collect:InvokeServer(1)
                        task.wait(0.3)
                        collect:InvokeServer(1)
                        task.wait(0.3)
                    end
                    if toggleShovelActive then
                        toggleShovelActive:FireServer(false)
                    end
                end
            end
        end
    end
    task.wait(1)
